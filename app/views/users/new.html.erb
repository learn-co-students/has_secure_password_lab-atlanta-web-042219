<h1>New User Form</h1>
<% if @user.errors %>
<% @user.errors.full_messages.each do |m| %>
  <%= m %>
  <% end %>
<% end %>
<%= form_for @user do |f| %>
  <%= f.label "Username: "%>
  <%= f.text_field :username %>
  <%= f.label "Password: "%>
  <%= f.password_field :password %>
    <%= f.label "Password Again: "%>
  <%= f.password_field :password_confirmation %>
  <%= f.submit %>
<% end %>